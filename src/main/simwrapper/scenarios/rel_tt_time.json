{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "data": {"url": "http://localhost:8000/base/rel_tt_time.csv"},
  "mark": "line",
  "encoding": {
    "x": {
      "timeUnit": "hoursminutes",
      "scale": { "domainMin": {"hours": 0}, "domainMax":{"hours": 23,"minutes": 45}},
      "field": "time",
      "type": "quantitative",
      "title": "Time",
      "axis": {
        "values":[{"hours": 4},{"hours": 8},{"hours": 12},{"hours": 16},{"hours": 20}]
      }
    },
    "y": {
      "field": "y",
      "type": "quantitative",
      "title": "Rel. travel time",
      "scale" : {"domain": [0.88, 1]}
    },
    "color": {
      "field":"scenario", "type" : "nominal"
    }
  }
}